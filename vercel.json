{
    "version": 2,
    "builds": [
        {
            "src": "/*.php",
            "use": "@vercel/node"
        }
    ],
    "routes": [
        {
            "src": "/api/(.*)",
            "dest": "/api_cotes",
            "methods": ["GET", "POST"],
            "headers": {
            "Content-Type": "application/json"
            }
        },
        {
            "src": "/(.*)",
            "dest": "/api_cotes.php"
        }
    ]
}
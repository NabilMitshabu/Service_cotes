{
    "functions": {
      "api/*.php": {
        "runtime": "vercel-php@0.5.0"
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api/$1.php",
        "methods": ["GET", "POST"],
        "headers": {
          "Content-Type": "application/json"
        }
      }
    ]
  }
  
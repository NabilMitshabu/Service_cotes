{
    "functions": {
      "*.php": {
        "runtime": "vercel-php@0.5.0"
      }
    },
    "routes": [
      {
        "src": "/api/(.*)",
        "dest": "/api_cotes.php",
        "methods": ["GET", "POST"],
        "headers": {
          "Content-Type": "application/json"
        }
      },
      {
        "src": "/(.*)",
        "dest": "/api_cotes.php"
      }
    ]
  }
  